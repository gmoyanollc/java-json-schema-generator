<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSPY v5 rel. 4 U (http://www.xmlspy.com) by Newton C. Lim (Northrop Grumman I T) -->
<xs:schema targetNamespace="urn:disa:udop.tracks:1.0:types" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:disa:udop.tracks:1.0:types" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="Track.xsd"/>
	<xs:complexType name="Missile">
		<xs:complexContent>
			<xs:extension base="C2TrackDetails">
				<xs:sequence>
					<xs:element name="Name" type="MissileNameType" minOccurs="0"/>
					<xs:element name="CorrelatedID" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>A Track ID this Missile track is correlated with.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EmergencyIndicator" type="xs:boolean" minOccurs="0"/>
					<xs:element name="DropPointIndicator" type="xs:boolean" minOccurs="0"/>
					<xs:element name="ForceTellIndicator" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>MIL-STD-6016  J3.0 Reference Point Msg _ this indicator causes track/point data to be forced through 		controllable receive filters.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpaceSpecificType" type="xs:short" minOccurs="0"/>
					<xs:element name="SpacePlatform" type="xs:short" minOccurs="0"/>
					<xs:element name="SpacePlatformActivity" type="xs:short" minOccurs="0"/>
					<xs:element name="Burst" type="BurstType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Expresses the type of burst from a nuclear detonation, and is used only with point type = 0, point amp = 4. 	(Reference Point Extension Word, MIL-STD-6016).</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EstimatedYield" type="EstimatedYieldType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>An estimate of the yield of a nuclear detonation expressed in megatons, and is used only with Point Type = 0, Point Amp = 4. (Reference Point Extension Word, MIL-STD 6016).</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BallisticMissileBeta" type="xs:float" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>  Expresses 	measured atmospheric drag effects on a ballistic missile with a constant reference area.  Beta is defined as the mass of the object divided by the product of its coefficient of drag and reference area. </definition>
								<comment>This field is only interpreted when boost indicator field is set to value 0, not in Boost.</comment>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BallisticMissileAccelerate" type="xs:float" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<comment>This field is only interpreted when boost indicator is set to value1, boost.</comment>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AltitudeSource" type="xs:float" minOccurs="0"/>
					<xs:element name="Altitude2" type="xs:float" minOccurs="0"/>
					<xs:element name="Cancelled" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Cancellation status of the missile track</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Destroyed" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Information on whether or not the missile track has been destroyed.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ImpactFan" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Information on the missile track's impact fan setting.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ImpactCalculated" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Information on whether or not the  missile track's impact position is calculated.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Radar" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Information on whether the missile track is a radar or TES track.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Strategic" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Information on whether the missile track is strategic or intercontinental.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpaceLaunch" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>Information on whether the missile track is a space launch vehicle.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OriginalXref" type="xs:string" minOccurs="0"/>
					<xs:element name="MissileUniqueIdentifier" type="MissileUniqueIdentifier" minOccurs="0"/>
					<xs:element name="CorrelatedMuids" type="Correlated-RedundantMuidsType" minOccurs="0"/>
					<xs:element name="RedundantMuids" type="Correlated-RedundantMuidsType" minOccurs="0"/>
					<xs:element name="ImpactReferencePoint" type="Impact-LaunchRefPointType" minOccurs="0"/>
					<xs:element name="LaunchReferencePoint" type="Impact-LaunchRefPointType" minOccurs="0"/>
					<xs:element name="ReceivedTrackNumbers" type="ReceivedDynamicIdentifiers" minOccurs="0"/>
					<xs:element name="SpaceAmplification" type="Character" minOccurs="0"/>
					<xs:element name="SpaceAmplificationConfidence" type="MissileAmpConfType" default="NoStatement" minOccurs="0"/>
					<xs:element name="TESTrackIdentifier" type="xs:string" minOccurs="0"/>
					<xs:element name="MissileVehicle" type="MissileVehicle" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Correlated-RedundantMuidsType">
		<xs:annotation>
			<xs:documentation>
				<definition>Correlated/Redundant missile-unique identifier information in an array of missile-unique identifiers.</definition>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MissileID" type="MissileUniqueIdentifier" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Impact-LaunchRefPointType">
		<xs:complexContent>
			<xs:extension base="EventDetails">
				<xs:sequence>
					<xs:element name="Time" type="xs:dateTime"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="ForceTellType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="NoStatement"/>
			<xs:enumeration value="ForceTellStatus"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BurstType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="NoStatement"/>
			<xs:enumeration value="Unknown"/>
			<xs:enumeration value="GroundSurface"/>
			<xs:enumeration value="Underground"/>
			<xs:enumeration value="WaterSurface"/>
			<xs:enumeration value="UnderWater"/>
			<xs:enumeration value="Space"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EstimatedYieldType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="NoStatement"/>
			<xs:enumeration value="Small"/>
			<xs:enumeration value="Medium"/>
			<xs:enumeration value="Large"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MissileAmpConfType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="NoStatement"/>
			<xs:enumeration value="ZeroToThirtyThree"/>
			<xs:enumeration value="ThirtyFourToSixtySix"/>
			<xs:enumeration value="SixtySevenToNinty"/>
			<xs:enumeration value="NintyOneToNintySeven"/>
			<xs:enumeration value="GreaterThanNintyEight"/>
			<xs:enumeration value="Type6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MissileNameType">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="26"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="MissileVehicle">
		<xs:sequence>
			<xs:element name="Area" type="xs:double" minOccurs="0"/>
			<xs:element name="BurnoutAltitude" type="xs:double" minOccurs="0"/>
			<xs:element name="BurnTime" type="xs:double" minOccurs="0"/>
			<xs:element name="Drag" type="xs:double" minOccurs="0"/>
			<xs:element name="DryMass" type="xs:double" minOccurs="0"/>
			<xs:element name="FlowRate" type="xs:double" minOccurs="0"/>
			<xs:element name="FuelMass" type="xs:double" minOccurs="0"/>
			<xs:element name="SpecificImpulse" type="xs:double" minOccurs="0"/>
			<xs:element name="Thrust" type="xs:double" minOccurs="0"/>
			<xs:element name="VehicleType" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
