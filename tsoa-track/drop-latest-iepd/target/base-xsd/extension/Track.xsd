					<xs:element ref="trk:ForceCode" minOccurs="0"/>
					<xs:element ref="lib:MotionModelCode" minOccurs="0"/>
					<xs:element ref="nc:BinaryURI" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="trk:AmbiguityReasonText" minOccurs="0"/>
					<xs:element ref="lib:DataAmplification" minOccurs="0"/>
					<xs:element ref="trk:Identifiers" minOccurs="0"/>
					<xs:element ref="mo:MILSTD2525-C-SIDC-Code" minOccurs="1"/>
					<xs:element ref="trk:RemarksList" minOccurs="0"/>
					<xs:element ref="trk:Targeting" minOccurs="0"/>
					<xs:element ref="trk:CrossReference" minOccurs="0"/>
					<xs:element ref="trk:CurrentEventGUID" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
