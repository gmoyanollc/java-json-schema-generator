	<xs:complexType name="TrackAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about a
				Track.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="structures:AugmentationType">
				<xs:sequence>
					<xs:element ref="glib:EnvironmentCategoryCode" minOccurs="0"/>
					<xs:element ref="trk:Threat" minOccurs="0"/>
					<xs:element ref="trk:TacticalTrainingCode" minOccurs="0"/>
					<xs:element ref="trk:TrackScope" minOccurs="0"/>
