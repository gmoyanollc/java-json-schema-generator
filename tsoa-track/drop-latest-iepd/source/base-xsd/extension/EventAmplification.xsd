	<xs:complexType name="AcousticEventAmplificationDetails">
		<xs:complexContent>
			<xs:extension base="EventAmplificationDetails">
				<xs:sequence>
					<xs:element name="AcousticSourceCode" type="AcousticSourceCode">
						<xs:annotation>
							<xs:documentation>
								<definition>OS-OTG: Source code defined in OPNAVINST S3431.3</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcousticFreq" type="AcousticFrequencyMeasure">
						<xs:annotation>
							<xs:documentation>
								<definition>OS-OTH: Fundamental frequency of the source being reported.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Harmonics" type="HarmonicMeasure">
						<xs:annotation>
							<xs:documentation>
								<definition>OS-OTH: Harmonic in descending order.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RPM" type="RevolutionsPerMinuteMeasure" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>OS-OTH: Revolutions Per Minute of the source being reported.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TurnsPerKnot" type="TurnsPerKnotMeasure" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>OS-OTH: Turns per Knot as reported.</definition>
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcousticSensorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>The sensor code of the detecting acoustic sensor from OS-OTH Entry List 1104 (Sensor Codes).</definition>
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="2"/>
								<xs:maxLength value="6"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="DetectionStatus" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								<definition>OS-OTG: field 9 of SIGNA line.  US-MTF: SIGNA line field 9 and provides fixed list of value values.</definition>
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="4"/>
								<xs:maxLength value="13"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="DateTimeGroup" type="xs:dateTime" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="AcousticSourceCode">
		<xs:annotation>
			<xs:documentation>
				<definition>OS-OTG: Source code defined in OPNAVINST S3431.3</definition>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AcousticFrequencyMeasure">
		<xs:annotation>
			<xs:documentation>
				<definition> The frequency of the acoustic source  being reported. </definition>
			</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="Decimal8Max">
				<xs:attribute name="measureUnitCode" use="required">
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:enumeration value="HZ"/>
							<xs:enumeration value="KHZ"/>
							<xs:enumeration value="MHZ"/>
							<xs:enumeration value="GHZ"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="HarmonicMeasure">
		<xs:annotation>
			<xs:documentation>
				<definition> The harmonic in descending order. </definition>
				<comment>OS-OTH: 1-2 digit combination separated by commas, 1-22NS.</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="22"/>
			<xs:pattern value="[0-9]{1,2}(,[0-9]{1,2})*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RevolutionsPerMinuteMeasure">
		<xs:annotation>
			<xs:documentation>
				<definition> The revolution per minute being reported. </definition>
				<comment>OS-OTH: 1-4 digits combination, decimal permitted (1-4NS).</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="4"/>
			<xs:pattern value="[0-9](4)[.]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TurnsPerKnotMeasure">
		<xs:annotation>
			<xs:documentation>
				<definition> The number of turns per knot being reported. </definition>
				<comment>OS-OTH: 1-5 digits combination, decimal permitted (1-5NS).</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
			<xs:pattern value="[0-9](5)[.]"/>
		</xs:restriction>
	</xs:simpleType> -->


