						<xs:element ref="tsoa:NickName" minOccurs="0"/>
						<xs:element ref="tsoa:SystemTrackNumber" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
	</xs:complexType>

