	<xs:complexType name="TrackAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about a Track.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="structures:AugmentationType">
				<xs:sequence>
					<xs:element ref="glib:EnvironmentCategoryCode" minOccurs="0"/>
					<xs:element ref="tsoa:Threat" minOccurs="0"/>
					<xs:element ref="tsoa:TacticalTrainingCode" minOccurs="0"/>
