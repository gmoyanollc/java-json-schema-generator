	<!-- replaced  with MilOps 
	<xs:complexType name="MilStd2525SymbolDetails">
		<xs:sequence>
			<xs:element name="MilStd2525SymbolId" type="MilStd2525SymbolIdentifier"/>
			<xs:group ref="MilStd2525SymbolModifiers" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="MilStd2525SymbolIdentifier">
		<xs:restriction base="xs:string">
      <xs:pattern value="[SGWIMOE]{1}[PUAFNSHGWMDLJKO]{1}[PAGSUFXZTMOVLIRNEQ\-]{1}[APCDXFSK\-]{1}[A-Z\-]{6}[ABCDEFGHMNP\-]{1}[A-Z\-]{1}[A-Z\-]{2}[AECGNSX\-]{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:group name="MilStd2525SymbolModifiers">
		<xs:sequence>
			<xs:element name="G" type="ModifierGText" minOccurs="0"/>
			<xs:element name="H" type="ModifierHText" minOccurs="0"/>
			<xs:element name="J" type="ModifierJText" minOccurs="0"/>
			<xs:element name="K" type="ModifierKText" minOccurs="0"/>
			<xs:element name="L" type="ModifierLText" minOccurs="0"/>
			<xs:element name="M" type="ModifierMText" minOccurs="0"/>
			<xs:element name="N" type="ModifierNText" minOccurs="0"/>
			<xs:element name="P" type="ModifierPText" minOccurs="0"/>
			<xs:element name="Q" type="ModifierQText" minOccurs="0"/>
			<xs:element name="T" type="ModifierTText" minOccurs="0"/>
			<xs:element name="V" type="ModifierVText" minOccurs="0"/>
			<xs:element name="W" type="ModifierWText" minOccurs="0"/>
			<xs:element name="X" type="ModifierXText" minOccurs="0"/>
			<xs:element name="Y" type="ModifierYText" minOccurs="0"/>
			<xs:element name="Z" type="ModifierZText" minOccurs="0"/>
			<xs:element name="AA" type="ModifierAAText" minOccurs="0"/>
		</xs:sequence>
	</xs:group> 
	<xs:simpleType name="ModifierGText">
		<xs:annotation>
			<xs:documentation>
				<definition>Staff Comments</definition>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierHText">
		<xs:annotation>
			<xs:documentation>
				<definition>Additional Information</definition>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierJText">
		<xs:annotation>
			<xs:documentation>
				<definition>Evaluation Rating</definition>
				<comment>Evaluation Rating is comprise of 1A Reliability (A,B,C,D,E,F) + 1N Credibility (1,2,3,4,5,6)</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[ABCDEF]{1}[1-6]{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierKText">
		<xs:annotation>
			<xs:documentation>
				<definition>Combat Effectiveness</definition>
				<comment>Combat Effectiveness is limited to 5 characters which suggests it may be coded or standardized values - however no details provided in Mil Std 2525B</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierLText">
		<xs:annotation>
			<xs:documentation>
				<definition>Signature Equipment</definition>
				<comment>Signature Equipment is only applicable to Equipment</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierMText">
		<xs:annotation>
			<xs:documentation>
				<definition>Higher Formation</definition>
				<comment>"Higher Echelon Command"</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="21"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierNText">
		<xs:annotation>
			<xs:documentation>
				<definition>Hostile (Enemy)</definition>
				<comment>Hostile (Enemy) is only applicable to Equipment</comment>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierPText">
		<xs:annotation>
			<xs:documentation>
				<definition>IFF/SIF</definition>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModifierQText">
		<xs:annotation>
			<xs:documentation>
