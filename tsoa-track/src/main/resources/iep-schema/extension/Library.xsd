	<xs:complexType name="TrademarkIdentifierType">
		<xs:annotation>
			<xs:documentation>A data type for the Trademark of the track being reported. Refer to
				NWP 1-03.41 (Classified Supplement) for a description of trademarks.
			</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="lib:TrademarkIdentifierSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="TrademarkIdentifierSimpleType">
		<xs:annotation>
			<xs:documentation> A data type for the Trademark of the track being reported. Refer to
				NWP 1-03.41 (Classified Supplement) for a description of trademarks. </xs:documentation>
			<xs:appinfo>
				<tsoa:comment>String Pattern: OS-OTG supports 1-20ANBS, USMTF has 1-30AN, COE
					supports up to 20 chars. Currently supporting 1-20ANBS.</tsoa:comment>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
