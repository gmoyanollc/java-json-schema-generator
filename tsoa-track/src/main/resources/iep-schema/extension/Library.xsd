	<xs:complexType name="InternationalRadioCallSignIdentifierType">
		<xs:annotation>
			<xs:documentation>A data type for the International Radio Call Sign (IRCS) of the track
				being reported. The IRCS is a communications identifier usually unique to a command
				or unit. </xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="lib:InternationalRadioCallSignIdentifierSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="InternationalRadioCallSignIdentifierSimpleType">
		<xs:annotation>
			<xs:documentation> A data type for the International Radio Call Sign (IRCS) of the track
				being reported. The IRCS is a communications identifier usually unique to a command
				or unit. </xs:documentation>
			<xs:appinfo>
				<tsoa:comment>String Pattern: OS-OTG supports 4-8AN; COE support up to 8 chars.
					Currently supporting 4-8AN.</tsoa:comment>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="4"/>
			<xs:maxLength value="8"/>
			<xs:pattern value="[A-Z0-9]{4,8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="EntityClassNameType">
		<xs:annotation>
			<xs:documentation>A data type for the class of ship being reported.</xs:documentation>
