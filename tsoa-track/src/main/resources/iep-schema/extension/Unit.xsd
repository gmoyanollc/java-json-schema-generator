  <xs:complexType name="UnitReferenceNumberType">
    <xs:annotation>
      <xs:documentation source="DoD-MIL-STD-6017B-VMF/unit-identification/urn"> A type for a
        Unit Reference Number (URN). "(URN) is a number used by units using VMF on an
        interface to uniquely identify friendly military units, broadcast networks, and
        multicast groups." </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="nm:UnitReferenceNumberType"/>
    </xs:simpleContent>
  </xs:complexType>
  
  <xs:element name="UnitReferenceNumber" type="tsu:UnitReferenceNumberType">
    <xs:annotation>
      <xs:appinfo source="JTCW-C-GW-2.0.0_TMS-API-Reference_2009/Track-Attribute/TRK_URN">
        JTCW API describes the track attribute TRK_URN as a "Unique Reference
        Number".</xs:appinfo>
      <xs:appinfo source="JTCW-C-GW-2.0.0_TMS-API-Reference_2009/TrackHeader.idl/lURN"> JTCW
        API UnitTrack.idl defines lURN.</xs:appinfo>
      <xs:appinfo source="TSOA-4.0.2.0.0/entity/connectors/jtcw_gateway_data_connector/src/mc2sa/jtcwgwdataconnector/UnitTrackConverter.java-358"> SOI JTCW connector has mapped UnitTrack.idl/lURN to
        SOI-Track-datatype/1.x/Track/Identifiers/UniqueReferenceCode</xs:appinfo>
      <xs:appinfo source="TSOA-4.0.2.0.0/entity/connectors/jtcw_gateway_data_connector/src/mc2sa/jtcwgwdataconnector/UnitTrackConverter.java-358"> SOI JTCW connector has mapped UnitTrack.idl/lURN to
        SOI-Track-datatype/1.x/Track/Unit/UnitReferenceNumber</xs:appinfo>
      <xs:documentation>A Unit Reference Number (URN)</xs:documentation>
    </xs:annotation>
  </xs:element>
  
